<% if (theme.links){ %>
    <div class="widget">
        <h3 class="widget-title"><%= __('link') %></h3>
        <div class="widget-body">
            <ul>
                <% for (var i in theme.links){ %>
                    <li>
                        <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
                    </li>
                <% } %>
            </ul>
        </div>
    </div>
<% } %>
